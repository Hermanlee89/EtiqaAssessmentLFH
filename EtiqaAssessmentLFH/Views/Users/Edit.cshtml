@model EtiqaAssessmentLFH.Models.userModel
@{
    ViewData["Title"] = "Create User";
}
<h2>Edit User</h2>

<form asp-controller="Users" asp-action="Edit" method="post">
    <input type="hidden" asp-for="Id" />
    <input type="hidden" asp-for="CreatedTime" />
    <div class="form-group">
        <label asp-for="Username" class="control-label" class="control-label"></label>
        <input asp-for="Username" readonly="readonly" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Mail" class="control-label"></label>
        <input asp-for="Mail" class="form-control" maxlength="50" />
    </div>
    <div class="form-group">
        <label asp-for="PhoneNo" class="control-label"></label>
        <input asp-for="PhoneNo" class="form-control" maxlength="12" />
    </div>
    <div class="form-group">
        <label asp-for="Skillsets" class="control-label"></label>
        <input asp-for="Skillsets" class="form-control" maxlength="200" />
    </div>
    <div class="form-group">
        <label asp-for="Hobby" class="control-label"></label>
        <input asp-for="Hobby" class="form-control" maxlength="200" />
    </div>
    
    <br />
    <button type="submit" class="btn btn-primary">Save</button>
    <br />
</form>

<a asp-controller="Users" asp-action="Index">Back to List</a>
